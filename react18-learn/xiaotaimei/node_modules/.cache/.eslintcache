g(res.data.data.film)\n          setFilmDetail({ ...res.data.data.film })\n    }\n\n    useEffect(() => {\n        loadFilmDetailInfo()\n        dispatch(switchTabBar({ status: false }))\n        return () => {\n          dispatch(switchTabBar({ status: true }))\n        }\n        // eslint-disable-next-line\n    }, [])\n\n    return (\n        <div className={ styles.film_detail_container }>\n             <div className={ styles.top_navBar }>\n                <NavBar onBack={() => navigate('/')}></NavBar>\n             </div>\n             <div className={ styles.center_content }>\n                 <div className={ styles.top_poster_cntainer }>\n                    <Image lazy width=\"100%\" height=\"100%\" fit='fill' src={ filmDetail.poster } />\n                 </div>\n                 <div className={ styles.film_desc_detail }>\n                     <div className={ styles.title_line }>\n                         <div className={ styles.title_label }>{ filmDetail.name }<span>{ filmDetail.item?.name }</span></div>\n                         <div className={ styles.film_score }>{ filmDetail.grade }分</div>\n                     </div>\n                     <div className={ styles.detail_item }>{ filmDetail.category }</div>\n                     <div className={ styles.detail_item }>{ filmDetail.premiereAt }上映</div>\n                     <div className={ styles.detail_item }>{ filmDetail.nation }｜{ filmDetail.runtime }分钟</div>\n                     <div className={ styles.detail_item }>{ filmDetail.category }</div>\n                     <div className={ styles.detail_item }>{ filmDetail.synopsis }</div>\n                 </div>\n                 <div className={ styles.actor_list }>\n                 <Swiper\n                    spaceBetween={10} slidesPerView={3.4} onSlideChange={() => console.log('slide change')}\n                    onSwiper={(swiper) => console.log(swiper)}\n                    >\n                        {\n                            filmDetail.actors?.map((item: any, index: any) => {\n                                return  <SwiperSlide key={ index }>\n                                <Image lazy width=\"100%\" height=\"135px\" fit='fill' src={ item.avatarAddress  } />\n                                <p>{ item.name }</p>\n                                <p>{ item.role }</p>\n                                </SwiperSlide>\n                            })\n                            \n                        }\n                    </Swiper>\n                 </div>\n             </div>\n             <div className={ styles.bottom_buy_btn }>\n                <Button block color='primary' size='middle' style={{ '--border-radius': 'none', fontSize: '14px' }}>\n                 选座购票\n                </Button>\n             </div>\n        </div>\n    )\n}\n","/Users/xinyonghu/Desktop/personalCode/react18-learn/xiaotaimei/src/views/cinemas/Cinema.tsx",[],["139"],"/Users/xinyonghu/Desktop/personalCode/react18-learn/xiaotaimei/src/views/login/Login.tsx",[],["140"],"/Users/xinyonghu/Desktop/personalCode/react18-learn/xiaotaimei/src/views/films/NowPlaying.tsx",[],["141"],"/Users/xinyonghu/Desktop/personalCode/react18-learn/xiaotaimei/src/views/cinemas/Search.tsx",[],["142"],"/Users/xinyonghu/Desktop/personalCode/react18-learn/xiaotaimei/src/views/films/ComingSoon.tsx",[],["143"],"/Users/xinyonghu/Desktop/personalCode/react18-learn/xiaotaimei/src/views/news/News.tsx",[],[],"/Users/xinyonghu/Desktop/personalCode/react18-learn/xiaotaimei/src/components/PageLoading.tsx",[],[],"/Users/xinyonghu/Desktop/personalCode/react18-learn/xiaotaimei/src/components/SwiperCom.tsx",[],[],"/Users/xinyonghu/Desktop/personalCode/react18-learn/xiaotaimei/src/components/CusVideo/CusVideo.tsx",["144"],[],{"ruleId":"145","severity":1,"message":"146","line":46,"column":8,"nodeType":"147","endLine":46,"endColumn":10,"suggestions":"148","suppressions":"149"},{"ruleId":"145","severity":1,"message":"150","line":9,"column":8,"nodeType":"147","en